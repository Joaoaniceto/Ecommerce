orage.getItem('cart')){\r\n           cart = JSON.parse(localStorage.getItem('cart'))\r\n       }\r\n       \r\n    \r\n       cart.map((c,i) => {\r\n           if (c._id === p._id){\r\n             cart[i].count = count;\r\n           } \r\n    });\r\n    \r\n    localStorage.setItem('cart',JSON.stringify(cart));\r\n    dispatch({\r\n        type:'ADD_TO_CART',\r\n        payload: cart\r\n    })\r\n    }\r\n    \r\n}\r\n\r\nconst handleremove = ()=>{\r\n    let cart = []\r\n\r\n\r\n    if (typeof window !== undefined){\r\n        if(localStorage.getItem('cart')){\r\n            cart = JSON.parse(localStorage.getItem('cart'));\r\n        }\r\n\r\n       cart.map((pro,i)=>{\r\n           if(pro._id === p._id){\r\n              cart.splice(i,1)\r\n           }\r\n       })\r\n       console.log(cart);\r\n\r\n    localStorage.setItem('cart',JSON.stringify(cart))\r\n    dispatch({\r\n        type:'ADD_TO_CART',\r\n        payload:cart\r\n    })\r\n\r\n    }\r\n}\r\n\r\nreturn(\r\n    <tbody className=\"\">\r\n          <tr>\r\n                  <td>\r\n                  <div style={{width:\"150px\",height:\"auto\"}}>\r\n                  {p.images.length ?\r\n                   (<ModalImage large={p.images.url} small={p.images[0].url} />)\r\n                    :\r\n                   (<ModalImage large={laptop} small={laptop} />)}\r\n                  </div>\r\n\r\n                  </td>\r\n                  <td>{p.title}</td>\r\n                  <td>{p.price}</td>\r\n                  <td>{p.brand}</td>\r\n                  <td className=\"text-center\">\r\n                  <input type=\"number\"\r\n                   className=\"form-control\"\r\n                   value={p.count}\r\n                   onChange={handleQuantity} >\r\n\r\n                  </input></td>\r\n                  <td>\r\n                      <select name=\"color\" onChange={handleChangeColor} className=\"form-control\">\r\n                          {p.color ? <option value={p.color}>{p.color}</option> : <option>Select</option>}\r\n                          {colors\r\n                          .filter((c)=> c !== p.color)\r\n                          .map((c)=><option key={c}>{c}</option>)}\r\n                      </select>\r\n                  </td>\r\n                  <td>{p.shiping === 'Yes' ? ( <CheckCircleOutlined className=\"text-sucesse\" />) : (<CloseCircleOutlined className=\"text-danger\" />)}</td>\r\n                  <td><CloseOutlined onClick={handleremove} className=\"text-danger pointer\" /></td>\r\n              \r\n          </tr>\r\n    </tbody>\r\n)\r\n}\r\n\r\n\r\nexport default ProductCardInCheckout;","C:\\Users\\OMEN\\Desktop\\newproject\\client\\src\\reducer\\drawerReducer.js",[],"C:\\Users\\OMEN\\Desktop\\newproject\\client\\src\\components\\Drawer\\SideDrawer.js",["438"],"import React from 'react'\r\nimport { Drawer,Button} from 'antd'\r\nimport {useSelector,useDispatch} from 'react-redux'\r\nimport {Link} from 'react-router-dom'\r\nimport laptop from '../../images/laptop.png'\r\n\r\n\r\n\r\n\r\nconst Sidedrawer = ()=>{\r\n\r\n    const {cart,drawer} = useSelector(state => ({...state}))\r\n    let  dispatch = useDispatch();\r\n\r\n    return(\r\n<Drawer \r\nclassName=\"text-center\"\r\ntitle={`Cart /${cart.length} Products`}\r\nplacement=\"right\"\r\nclosable={false}\r\n\r\nonClose={()=>{dispatch({type:'SET_VISIBLE',payload: false})}} \r\nvisible={drawer}>\r\n    {cart.map((p)=>(\r\n        <div key={p._id} className=\"row\">\r\n          <div className=\"col\">\r\n                {p.images[0] ?  \r\n                <>\r\n                <img alt=\"product\" style={{width:\"100%\",objectFit:\"cover\",height:'50px'}} src={p.images[0].url} /> \r\n                <p className=\"text-center bg-secondary text-light\">\r\n                    {p.title} x {p.count}\r\n                </p>\r\n                </>\r\n                :  <>\r\n                <img alt=\"default\" style={{width:\"100%\",objectFit:\"cover\",height:'50px'